<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <template class="BookmarkEditor" parent="GtkPopover">
    <child>
      <object class="GtkGrid">
        <property name="row-spacing">5</property>
        <property name="column-spacing">5</property>
        <child>
          <object class="GtkLabel" id="label">
            <property name="use-markup">1</property>
	          <property name="halign">center</property>
	          <property name="label">&lt;b&gt;Create Bookmark&lt;/b&gt;</property>
	          <layout>
              <property name="column">0</property>
	            <property name="row">0</property>
	            <property name="column-span">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkLabel">
            <property name="label">Name</property>
			      <layout>
              <property name="column">0</property>
			        <property name="row">1</property>
            </layout>
          </object>
		    </child>
		    <child>
          <object class="GtkEntry" id="name">
			      <layout>
              <property name="column">1</property>
			        <property name="row">1</property>
            </layout>
          </object>
		    </child>
		    <child>
          <object class="GtkLabel">
			      <property name="label">Description</property>
			      <layout>
              <property name="column">0</property>
			        <property name="row">2</property>
            </layout>
          </object>
		    </child>
		    <child>
          <object class="GtkEntry" id="description">
			      <layout>
              <property name="column">1</property>
			        <property name="row">2</property>
            </layout>
          </object>
		    </child>
		    <child>
          <object class="GtkLabel">
			      <property name="label">Url</property>
			      <layout>
              <property name="column">0</property>
			        <property name="row">3</property>
            </layout>
          </object>
		    </child>
		    <child>
          <object class="GtkEntry" id="url">
			      <layout>
              <property name="column">1</property>
			        <property name="row">3</property>
            </layout>
          </object>
		    </child>
		    <child>
          <object class="GtkLabel">
			      <property name="label">Tags</property>
			        <layout>
                <property name="column">0</property>
			          <property name="row">4</property>
              </layout>
          </object>
		    </child>
		    <child>
          <object class="GtkEntry" id="tags">
			      <layout>
              <property name="column">1</property>
			        <property name="row">4</property>
            </layout>
          </object>
		    </child>
		    <child>
          <object class="GtkButton" id="cancel">
            <property name="hexpand">0</property>
			      <property name="halign">start</property>
			      <property name="label">Cancel</property>
			      <layout>
              <property name="column">0</property>
			        <property name="row">5</property>
            </layout>
          </object>
		    </child>
		    <child>
          <object class="GtkButton" id="accept">
            <property name="hexpand">0</property>
			      <property name="halign">end</property>
			      <property name="label">Accept</property>
			      <property name="css-classes">suggested-action</property>
			      <layout>
              <property name="column">1</property>
			        <property name="row">5</property>
            </layout>
          </object>
		    </child>
      </object>
    </child>
  </template>
</interface>
